{
  // ***************
  // ** WORKSPACE **
  // ***************
  "workbench.settings.editor": "json",

  "workbench.commandPalette.history": 20,
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "welcomePage",
  "workbench.list.keyboardNavigation": "filter",
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.tips.enabled": true,
  // FILES

  "files.autoSave": "off",
  // ************
  // ** EDITOR **
  // ************

  // GIT
  "git.confirmSync": false,
  "git.inputValidation": "off",
  "diffEditor.renderSideBySide": true,
  // NPM
  "npm.enableScriptExplorer": true,
  "npm.packageManager": "yarn",
  "terminal.integrated.fontSize": 14,
  "explorer.confirmDragAndDrop": false,
  "breadcrumbs.enabled": true,
  "prettier.singleQuote": true,

  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  // ***************
  // ** LANGUAGES **
  // ***************
  // MARKDOWN
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  // GITLENSE
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.views.compare.files.layout": "list",

  // ESLINT
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "yarn",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "explorer.openEditors.visible": 0,
  "gitlens.hovers.currentLine.over": "line",
  "workbench.activityBar.visible": true,
  "window.autoDetectColorScheme": true,
  "workbench.preferredDarkColorTheme": "Popping and Locking",
  "workbench.preferredLightColorTheme": "Popping and Locking",
  "search.searchOnTypeDebouncePeriod": 450,
  "search,exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/build": true,
    "**/coverage": true,
    "**/out": true,
    "**/tmp": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/documentation": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDelete": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "git.enableSmartCommit": true,
  "tabnine.experimentalAutoImports": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.tabs.enabled": true,
  "security.workspace.trust.untrustedFiles": "open",
  "git.autofetch": true,
  "editor.inlineSuggest.enabled": true,
  "redhat.telemetry.enabled": false,
  "workbench.editorAssociations": {
    "*.html": "default"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.zoomLevel": -1
}
